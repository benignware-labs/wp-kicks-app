{
  "name": "wp-kicks-app",
  "version": "0.0.1",
  "description": "Wordpress theme",
  "main": "dist/main.js",
  "style": "dist/main.css",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/benignware-labs/wp-kicks-app.git"
  },
  "keywords": [
    "wordpress",
    "theme"
  ],
  "author": "Rafael Nowrotek",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/benignware-labs/wp-kicks-app/issues"
  },
  "homepage": "https://github.com/benignware-labs/wp-kicks-app#readme",
  "devDependencies": {
    "@wordpress/browserslist-config": "^2.2.2",
    "autoprefixer": "^9.1.5",
    "chokidar-cli": "^1.2.1",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.0",
    "postcss-focus-within": "^3.0.0",
    "query-ast": "^1.0.2",
    "rtlcss": "^2.4.0",
    "sass-extract": "^2.1.0",
    "sass-variable-parser": "^1.2.2",
    "scss-parser": "^1.0.3"
  },
  "rtlcssConfig": {
    "options": {
      "autoRename": false,
      "autoRenameStrict": false,
      "blacklist": {},
      "clean": true,
      "greedy": false,
      "processUrls": false,
      "stringMap": []
    },
    "plugins": [],
    "map": false
  },
  "browserslist": [
    "extends @wordpress/browserslist-config"
  ],
  "scripts": {
    "build:style": "node-sass style.scss style.css --include-path node_modules --output-style expanded && postcss -r style.css",
    "build:bootstrap": "node ./build.js",
    "build:style-editor": "node-sass style-editor.scss style-editor.css --output-style expanded && postcss -r style-editor.css",
    "build:style-editor-customizer": "node-sass style-editor-customizer.scss style-editor-customizer.css --output-style expanded && postcss -r style-editor-customizer.css",
    "build:rtl": "rtlcss style.css style-rtl.css",
    "build:print": "node-sass print.scss print.css --output-style expanded && postcss -r print.css",
    "build": "run-p \"build:*\"",
    "watch": "chokidar \"**/*.scss\" -c \"npm run build\" --initial"
  },
  "dependencies": {
    "bootstrap": "^4.3.1"
  }
}
